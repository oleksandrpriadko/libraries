<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CurvedImageView">
        <attr name="curveColor" format="color" />
    </declare-styleable>

    <declare-styleable name="AttachedTabsView">
        <attr name="marginTabs" format="dimension" />
        <attr name="curveColorTabs" format="color" />
        <attr name="areTabsOnTop" format="boolean" />
        <attr name="selectedItem" format="integer" />
        <attr name="floatingDrawable" format="reference|color" />
        <attr name="animationDuration" format="integer" />
    </declare-styleable>
    <declare-styleable name="TabState">
        <attr name="state_animating" format="boolean" />
    </declare-styleable>
</resources>